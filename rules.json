[
    {
        "root": "./deps/libcanvas/src/",
        "include-relative": ".",
        "include-dirs": [
        ],
        "files": [
            "*.h"
        ],
        "excluded-files": [
        ],
        "output":[
            {
                "template": "./templates/Class.hx.mustache",
                "rule": "file-per-class",
                "path": "src/libcanvas/{{class_name}}.hx",
                "types": {
                    "unsigned int" : "Int",
                    "const int": "Int",
                    "const char *": "String",
                    "const float": "Float",
                    "const bool": "Bool"
                }
            }
        ],
        "excluded-types":[
        ]
    }
]
